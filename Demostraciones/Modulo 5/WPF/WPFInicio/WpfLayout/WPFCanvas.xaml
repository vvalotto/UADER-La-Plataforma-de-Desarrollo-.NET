<Window x:Class="APSoftWPF.WPFCanvas"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WPFCanvas" Height="300" Width="300" Loaded="Window_Loaded">
    <Canvas Name="canvas1">
        <Rectangle Canvas.Left="40" Canvas.Top="39" Height="100" Name="rectangle1" Width="200" OpacityMask="#FF40D827" Fill="#FFCB1212" Panel.ZIndex="1" Opacity="1">
            <Rectangle.Stroke>
                <LinearGradientBrush EndPoint="1,0.5" StartPoint="0,0.5">
                    <GradientStop Color="Black" Offset="0" />
                    <GradientStop Color="#FF15CECE" Offset="1" />
                </LinearGradientBrush>
            </Rectangle.Stroke>
        </Rectangle>
        <Ellipse Canvas.Left="62" Canvas.Top="126" Height="100" Name="ellipse1" Stroke="Black" Width="100" Fill="#FF2BC72B" />
        <Image Canvas.Left="136" Canvas.Top="136" Height="81" Name="image1" Stretch="Fill" Width="80" Source="/APSoftWPF;component/Images/Leaf.bmp" />
    </Canvas>
</Window>
